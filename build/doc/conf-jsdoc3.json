{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "((^|\\/|\\\\)_)|((\\/|\\\\)node_modules)|((\\/|\\\\)vendor)"
    },
    "plugins": [
	    "./../../plugins/short-symbolic-links",
	    "./../../plugins/convert-atag-links-in-see-tag",
	    "./../../plugins/copydoc-tag-def",
	    "./../../plugins/copydoc-process"
	],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
		"navType ": "inline",
		"linenums": true,
		"theme": "cerulean",
